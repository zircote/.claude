{
  "model": "opus",
  "statusLine": {
    "type": "command",
    "command": "input=$(cat); cwd=$(echo \"$input\" | jq -r '.workspace.current_dir'); model=$(echo \"$input\" | jq -r '.model.display_name'); output_style=$(echo \"$input\" | jq -r '.output_style.name'); git_branch=\"\"; if git -C \"$cwd\" rev-parse --git-dir > /dev/null 2>&1; then git_branch=$(git -C \"$cwd\" -c core.fileMode=false -c advice.detachedHead=false rev-parse --abbrev-ref HEAD 2>/dev/null || echo \"\"); if [ -n \"$git_branch\" ]; then git_branch=\" | git:$git_branch\"; fi; fi; timestamp=$(date '+%H:%M:%S'); printf \"\\033[36m%s\\033[0m%s | \\033[35m%s\\033[0m | \\033[33m%s\\033[0m | \\033[32m%s\\033[0m\" \"$cwd\" \"$git_branch\" \"$model\" \"$output_style\" \"$timestamp\""
  },
